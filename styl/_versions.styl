.version
  &__container
    height 100%
    width 100%

  &__table
    width 100%
    height 100%
    padding 0.5em
    border-spacing 0.5em
    font-size 1vw

  &__cell
    background-color #535c68
    border-radius 0.5em
    text-align center

  &__cell:hover
    cursor pointer

  &__cell:active
    box-shadow 0 0.1em 0 rgba(0,0,0,0.4) inset

  &__up
    border: #009300 1px solid;

  &__down
    border: #b00 1px solid